<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="http://xmlns.jcp.org/xml/ns/persistence" version="2.1">

    <persistence-unit name="cpdbEntities" transaction-type="JTA">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>models.Household</class>
        <class>models.Person</class>
        <class>models.Personteam</class>
        <class>models.Team</class>
        <properties>
            <property name="eclipselink.jdbc.url" value="jdbc:oracle:thin:@//localhost:1521/XE"/>
            <property name="eclipselink.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
            <property name="eclipselink.jdbc.user" value="cpdb"/>
            <property name="eclipselink.jdbc.password" value="bjarne"/>
        </properties>
    </persistence-unit>
</persistence>
